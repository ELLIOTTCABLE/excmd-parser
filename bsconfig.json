{
  "name": "excmd-parser",
  "version": "0.0.1",
  "sources": [
    {
      "dir": "src",
      "generators": [
         { "name": "prepend-uax31", "edge": ["lexer.ml", ":", "uAX31.ml", "lexer.body.ml"] },
         { "name": "menhir-tokens", "edge": ["tokens.ml", "tokens.mli", ":", "parserAutomaton.mly", "tokens.tail.ml", "tokens.tail.mli"] },
         { "name": "menhir-lib", "edge": ["menhirLib.ml", "menhirLib.mli", ":", "parserAutomaton.mly"] },
         { "name": "menhir", "edge": ["parserAutomaton.ml", ":", "parserAutomaton.mly", "tokens.ml"] }
      ]
    }
  ],
  "ppx-flags": [
    "ppx-sedlex/ppx.js"
  ],
  "generators": [
      {
         "name": "menhir",
         "command": "menhir $src_root_dir/src/parserAutomaton.mly --external-tokens Tokens --log-automaton 1 --log-code 1 --log-grammar 1 --trace"
      },
      {
         "name": "menhir-tokens",
         "command": "menhir --only-tokens $src_root_dir/src/parserAutomaton.mly --base $src_root_dir/src/tokens && tail -n +2 $src_root_dir/src/tokens.tail.ml >> $src_root_dir/src/tokens.ml && tail -n +2 $src_root_dir/src/tokens.tail.mli >> $src_root_dir/src/tokens.mli"
      },
      {
         "name": "menhir-lib",
         "command": "cp \"$$(menhir --suggest-menhirLib)\"/*.ml* $src_root_dir/src/"
      },
      {
         "name": "prepend-uax31",
         "command": "cp $src_root_dir/src/uAX31.ml ${out} && tail -n +2 $src_root_dir/src/lexer.body.ml >> ${out}"
      }
  ],
  "bs-dependencies": [
    "bs-sedlex",
    "@elliottcable/bs-uchar",
    "@elliottcable/bs-gen"
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bsc-flags": [
    "-bs-super-errors",
    "-bs-no-version-header"
  ],
  "refmt": 3
}
