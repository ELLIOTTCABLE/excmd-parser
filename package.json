{
  "name": "excmd-parser",
  "version": "0.0.1",
  "description": "Tridactyl's parser for Vi-style command-line, rc-files, and vimscript",
  "main": "index.js",
  "scripts": {
    "build:tokens": "menhir --only-tokens src/parserAutomaton.mly --base src/tokens",
    "build:parser": "npm run build:tokens && menhir --log-automaton 1 --log-code 1 --log-grammar 1 --trace --external-tokens Tokens src/parserAutomaton.mly",
    "build": "npm run build:parser && bsb -make-world",
    "clean": "bsb -clean-world",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ELLIOTTCABLE/excmd-parser.git"
  },
  "author": "ELLIOTTCABLE <npm@ell.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ELLIOTTCABLE/excmd-parser/issues"
  },
  "homepage": "https://github.com/ELLIOTTCABLE/excmd-parser#readme",
  "dependencies": {
    "bs-platform": "^4.0.2",
    "bs-sedlex": "^1.99.4-pre.8"
  },
  "devDependencies": {
    "ppx-sedlex": "^1.99.4-pre.8"
  }
}
