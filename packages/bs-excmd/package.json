{
   "name": "bs-excmd",
   "version": "1.0.0",
   "description": "OCaml component of Tridactyl's parser for Vi-style command-line, rc-files, and vimscript",
   "main": "lib/interface.js",
   "types": "lib/interface.d.ts",
   "files": [
      "bsconfig.json",
      "tsconfig.json",
      "docs",
      "lib",
      "shim",
      "src",
      "test",
      "!lib/bs",
      "!**/dune",
      "!**/.merlin"
   ],
   "scripts": {
      "build:scripts": "cd scripts && bsb -make-world",
      "build:doc": "make build-doc",
      "build": "make build-ml",
      "watch": "bsb -make-world -w",
      "clean": "make clean",
      "clean:all": "make clean-all",
      "prepare": "npm-run-all clean build",
      "test:js": "jest",
      "test:js:coverage": "cd ../../ && jest --coverage",
      "test:ml": "dune runtest",
      "test:ml:promote": "dune promote",
      "test": "npm-run-all build --parallel test:js test:ml",
      "format:js": "prettier --ignore-path .gitignore --write 'src/**/*.ts' 'src/**/*.js' 'test/**/*.js' 'examples/**/*.ts'",
      "format:ml": "make format-ml",
      "format": "npm-run-all 'format:*'",
      "pre-commit": "npm run test:ml"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/ELLIOTTCABLE/excmd.js.git"
   },
   "author": "ELLIOTTCABLE <npm@ell.io>",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/ELLIOTTCABLE/excmd.js/issues"
   },
   "homepage": "https://github.com/ELLIOTTCABLE/excmd.js#readme",
   "dependencies": {
      "@babel/runtime": "^7.5.5",
      "@elliottcable/bs-result": "^12.0.0",
      "bs-deriving": "^44.1.0",
      "bs-gen": "^0.5.3",
      "bs-platform": "^5.2.1",
      "bs-sedlex": "^20.0.0",
      "bs-uchar": "^2.0.0",
      "fast-text-encoding": "^1.0.0",
      "ocaml-string-convert": "0.0.3"
   },
   "devDependencies": {
      "@babel/cli": "^7.5.5",
      "@babel/core": "^7.5.5",
      "@babel/preset-env": "^7.5.5",
      "@types/terminal-kit": "^1.28.0",
      "babel-jest": "^24.9.0",
      "jest": "^24.9.0",
      "lerna": "^3.20.2",
      "lint-staged": "^10.0.0-beta.14",
      "npm-run-all": "^4.1.5",
      "ppx-deriving": "^44.1.0",
      "ppx-sedlex": "^20.0.0",
      "prettier": "^1.18.2",
      "terminal-kit": "^1.31.1",
      "typedoc": "^0.15.0",
      "typescript": "^3.5.3"
   }
}
