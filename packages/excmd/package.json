{
   "name": "excmd",
   "version": "1.0.0",
   "description": "JavaScript interface to Tridactyl's parser for Vi-style command-line, rc-files, and vimscript",
   "main": "lib/interface.js",
   "types": "lib/interface.d.ts",
   "files": [
      "bsconfig.json",
      "tsconfig.json",
      "docs",
      "lib",
      "shim",
      "src",
      "test",
      "!lib/bs",
      "!**/dune",
      "!**/.merlin"
   ],
   "scripts": {
      "build:bs": "bsb -make-world",
      "build:dts": "tsc -p tsconfig.gen-declarations.json",
      "build:examples": "babel --verbose examples --extensions '.ts' --out-dir examples",
      "build:js": "babel --verbose src --extensions '.ts,.js' --ignore '**/*.d.ts' --out-dir lib",
      "build": "npm-run-all build:bs build:js",
      "clean": "rm -rf lib/ && bsb -clean-world",
      "prepare": "npm run build"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/ELLIOTTCABLE/excmd.js.git"
   },
   "author": "ELLIOTTCABLE <npm@ell.io>",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/ELLIOTTCABLE/excmd.js/issues"
   },
   "homepage": "https://github.com/ELLIOTTCABLE/excmd.js#readme",
   "devDependencies": {
      "@babel/cli": "^7.5.5",
      "@babel/core": "^7.5.5",
      "@babel/plugin-transform-runtime": "^7.5.5",
      "@babel/preset-env": "^7.5.5",
      "@babel/preset-typescript": "^7.3.3",
      "@types/terminal-kit": "^1.28.0",
      "babel-jest": "^24.8.0",
      "bs-excmd": "^1.0.0",
      "bs-platform": "^5.2.1",
      "jest": "^24.8.0",
      "lerna": "^3.20.2",
      "lint-staged": "^10.0.0-beta.14",
      "npm-run-all": "^4.1.5",
      "prettier": "^1.18.2",
      "terminal-kit": "^1.31.1",
      "typedoc": "^0.15.0",
      "typescript": "^3.5.3"
   }
}
