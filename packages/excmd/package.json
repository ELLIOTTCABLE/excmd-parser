{
   "name": "excmd",
   "version": "1.0.0",
   "description": "JavaScript interface to Tridactyl's parser for Vi-style command-line, rc-files, and vimscript",
   "main": "lib/interface.js",
   "types": "lib/interface.d.ts",
   "files": [
      "bsconfig.json",
      "tsconfig.json",
      "src",
      "!src/**/*.d.ts*",
      "lib",
      "dist",
      "test",
      "examples",
      "!lib/bs",
      "!**/dune",
      "!**/.merlin"
   ],
   "scripts": {
      "build:bs": "bsb -make-world",
      "build:dist": "rollup --config",
      "build:doc": "typedoc ./src",
      "build:dts": "tsc -p tsconfig.gen-declarations.json",
      "build:examples": "babel --root-mode upward --verbose examples --extensions '.ts' --out-dir examples",
      "build:js": "babel --root-mode upward --verbose src --extensions '.ts,.js' --ignore '**/*.d.ts' --copy-files --out-dir lib",
      "build": "npm-run-all build:bs build:dts build:js build:dist build:examples",
      "clean": "rm -rf lib/ dist/ && bsb -clean",
      "prepare": "npm-run-all clean build",
      "test": "jest",
      "test:coverage": "cd ../../ && jest --coverage"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/ELLIOTTCABLE/excmd.js.git"
   },
   "author": "ELLIOTTCABLE <npm@ell.io>",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/ELLIOTTCABLE/excmd.js/issues"
   },
   "homepage": "https://github.com/ELLIOTTCABLE/excmd.js#readme",
   "devDependencies": {
      "@babel/cli": "^7.5.5",
      "@babel/core": "^7.5.5",
      "@babel/plugin-transform-runtime": "^7.5.5",
      "@babel/preset-env": "^7.5.5",
      "@babel/preset-typescript": "^7.3.3",
      "@rollup/plugin-node-resolve": "^7.0.0",
      "@types/terminal-kit": "^1.28.0",
      "@wessberg/rollup-plugin-ts": "^1.2.13",
      "babel-jest": "^24.9.0",
      "bs-excmd": "^1.0.0",
      "bs-platform": "^5.2.1",
      "jest": "^24.9.0",
      "lerna": "^3.20.2",
      "lint-staged": "^10.0.0-beta.14",
      "node-jq": "^1.11.0",
      "npm-run-all": "^4.1.5",
      "ocaml-string-convert": "0.0.3",
      "prettier": "^1.18.2",
      "rollup": "^1.29.0",
      "terminal-kit": "^1.31.1",
      "typedoc": "^0.15.0",
      "typescript": "^3.5.3"
   }
}
